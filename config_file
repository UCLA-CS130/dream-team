port 1024;  # This is also a comment.

path /static StaticHandler {
  root static_src;
}

path / StaticHandler {
  root static_src;
}

path /echo EchoHandler {}

path /status StatusHandler {}

path /proxy ProxyHandler {
  host ucla.edu;
  port 8080;
}

# Default response handler if no handlers match.
default BadRequestHandler {}
